[DEFAULT]
bantime = 3600
findtime = 600
maxretry = 5

[wordpress]
enable = true
logpath = /var/www/html/wp-debug.log
port = 443
filter = wordpress
action = iptables-multiport[name=wordpress, port="443"]

[vsftpd]
enabled = true
logpath = /var/log/vsftpd.log
port = 21
filter = vsftpd
action = iptables-multiport[name=vsftpd, port="21"]
